const majorityElement = (nums) => {

    //Boyerâ€“Moore majority vote algorithm
    let counter = 1
    let element = nums[0]

    for (let i = 1; i < nums.length; i++) {

        // if (nums[i] == element) {
        //     counter++
        // }
        // else counter--

        if (!counter) { //falsy when counter==0
            element = nums[i]
        }
        counter += nums[i] == element ? 1 : -1
    }

    return element
};
