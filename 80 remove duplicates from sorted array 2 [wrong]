const removeDuplicates = (nums) => {

    let currIndex = 1

    for (let i = 1; i < nums.length; i++) {
        if (nums[i] != nums[i - 1]) {
            nums[currIndex] = nums[i]
            currIndex++
        }

        if (nums[i] == nums[i - 1]) {
            if (nums[i] != nums[i - 2]) {
                nums[currIndex] = nums[i]
                currIndex++
            }
        }
    }

    return currIndex
};
